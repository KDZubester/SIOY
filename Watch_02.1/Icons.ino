const unsigned char * getIcon(int iconNum) {
  switch (iconNum) {
    case 0:
      return battery;
      break;
    case 1:
      return battery_one_bar;
      break;
    case 2:
      return battery_two_bars;
      break;
    case 3:
      return battery_three_bars;
      break;
    case 4:
      return right_arrow;
      break;
    case 5:
      return left_arrow;
      break;
    case 6:
      return up_arrow;
      break;
    case 7:
      return down_arrow;
      break;
    default:
      return battery;
      break;
  }
}




  
